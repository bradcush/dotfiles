{
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "javascript",
        "javascriptreact",
        "json",
        "markdown",
        "Markdown",
        "python",
        "typescript",
        "typescriptreact"
    ],
    "codeLens.enable": true, // Contextual information
    "diagnostic.errorSign": "🚫",
//  "diagnostic.errorSign": "❌",
    "diagnostic.hintSign": "📎",
    "diagnostic.infoSign": "💁",
    "diagnostic.warningSign": "🔥",
//  "diagnostic.warningSign": "⚠️",
//  "prettier.eslintIntegration": true, // Use prettier-eslint
    "prettier.requireConfig": true, // Require config to format
    "python.formatting.provider": "black",
    "python.jediEnabled": false, // Use Microsoft PLS
    "suggest.enablePreview": true, // Preview for completeopt
    "suggest.maxCompleteItemCount": 10, // Max items shown
    "typescript.preferences.quoteStyle": "single"
}
