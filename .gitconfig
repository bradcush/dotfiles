[core]
	excludesfile = /Users/bcushing/.gitignore_global
[user]
	name = Bradley Cushing
	email = bradleycushing@gmail.com
	# Singing key for macOS
	signingkey = 9E1C2A90260664D0
	# Singing key for Arch Linux
  # signingkey = 0DC07BB23793C014
[commit]
	gpgsign = true
[tag]
	forceSignAnnotated = true
[rebase]
	autosquash = true
[pager]
	branch = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[init]
	defaultBranch = main
[url "git@github.com:"]
	# Forces ssh useful private plugins
	insteadOf = https://github.com/
