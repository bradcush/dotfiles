[core]
	excludesfile = /Users/bcushing/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = Bradley Cushing
	email = bradley.cushing@dashlane.com
	signingkey = 78115DED2DCC4291E175509059F654E413DC2247
[commit]
	gpgSign = true
	template = /Users/bcushing/.stCommitMsg
[tag]
	forceSignAnnotated = true

[rebase]
	autoaquash = true
	autosquash = true
[pager]
	branch = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
